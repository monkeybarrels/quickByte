{
  "name": "@quickbyte/pipelines",
  "version": "0.1.1",
  "description": "Flexible data transformation pipeline for QuickByte",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && tsc --build tsconfig.cjs.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm run build && pnpm test",
    "version:patch": "pnpm version patch",
    "version:minor": "pnpm version minor",
    "version:major": "pnpm version major",
    "version:pre": "pnpm version prerelease",
    "version:prepatch": "pnpm version prepatch",
    "version:preminor": "pnpm version preminor",
    "version:premajor": "pnpm version premajor"
  },
  "keywords": [
    "quickbyte",
    "pipeline",
    "data",
    "transformation",
    "etl"
  ],
  "author": "Jim Livingston",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/monkeybarrels/quickByte.git"
  },
  "bugs": {
    "url": "https://github.com/monkeybarrels/quickbyte/issues"
  },
  "homepage": "https://github.com/monkeybarrels/quickbyte#readme",
  "peerDependencies": {
    "@quickbyte/core": "workspace:*",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.28",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "@types/bun": "latest",
    "axios-mock-adapter": "^1.21.5"
  },
  "dependencies": {
    "@quickbyte/core": "^0.1.10",
    "axios": "^1.8.4",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "mongodb": "^6.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}